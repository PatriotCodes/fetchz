{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "chrome": "42",
          "edge": "14",
          "safari": "11"
        }
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ]
  ],
  "env": {
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": 8
            }
          }
        ]
      ]
    }
  }
}
